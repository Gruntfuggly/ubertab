{
    "name": "ubertab",
    "displayName": "Ubertab",
    "description": "Context sensitive tabbing",
    "icon": "icon.png",
    "version": "0.0.7",
    "publisher": "Gruntfuggly",
    "repository": "https://github.com/Gruntfuggly/ubertab",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "ubertab.enable",
                "title": "Ubertab: Enable"
            },
            {
                "command": "ubertab.disable",
                "title": "Ubertab: Disable"
            }
        ],
        "keybindings": [
            {
                "key": "tab",
                "command": "ubertab.tabForward",
                "when": "editorTextFocus && ubertab-enabled && !inSnippetMode && !suggestWidgetVisible && !editorHasSelection"
            },
            {
                "key": "shift+tab",
                "command": "ubertab.tabBackward",
                "when": "editorTextFocus && ubertab-enabled && !inSnippetMode && !suggestWidgetVisible && !editorHasSelection"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "Ubertab",
            "properties": {
                "ubertab.enabled": {
                    "type": "boolean",
                    "default": true,
                    "markdownDescription": "Enable Ubertab"
                },
                "ubertab.showInStatusBar": {
                    "type": "boolean",
                    "default": true,
                    "markdownDescription": "Show status bar button"
                },
                "ubertab.shouldIndent": {
                    "type": "boolean",
                    "default": true,
                    "markdownDescription": "If true, pressing tab at the start of a line indents"
                },
                "ubertab.tabOutOfBraces": {
                    "type": "boolean",
                    "default": true,
                    "markdownDescription": "Tab should jump out of matching braces"
                },
                "ubertab.tabOutOfBrackets": {
                    "type": "boolean",
                    "default": true,
                    "markdownDescription": "Tab should jump out of matching brackets"
                },
                "ubertab.tabOutOfSquareBrackets": {
                    "type": "boolean",
                    "default": true,
                    "markdownDescription": "Tab should jump out of matching square brackets"
                },
                "ubertab.tabOutOfAngleBrackets": {
                    "type": "boolean",
                    "default": true,
                    "markdownDescription": "Tab should jump out of matching angle brackets"
                }
            }
        }
    },
    "devDependencies": {
        "vscode": "^1.0.0"
    },
    "__metadata": {
        "id": "687f0278-d187-4794-90f6-be61b4689b7a",
        "publisherDisplayName": "Gruntfuggly",
        "publisherId": "d4906d2e-f2ee-492d-9c7c-02b6160599ec"
    }
}